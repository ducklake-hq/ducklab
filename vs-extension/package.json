{
  "name": "ducklab",
  "displayName": "Ducklab",
  "description": "Interactive SQL/Python Notebooks. Powered by DuckDB",
  "version": "0.5.0",
  "license": "Apache-2.0",
  "publisher": "ducklake",
  "icon": "./assets/logo_dark.png",
  "homepage": "https://ducklake.io/ducklab",
  "repository": "https://github.com/ducklake-hq/ducklab",
  "scripts": {
    "dev": "rollup --watch --config rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "build": "rollup --config rollup.config.ts --configPlugin @rollup/plugin-typescript",
    "package": "./package.sh",
    "publish": "find ./dist -name ducklab*.vsix -exec sh -c 'npx vsce publish --packagePath {} -p $VSCE_PAT || exit 1' \\;",
    "lint": "eslint src --ext .ts,.tsx",
    "test": "jest"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "keywords": [
    "notebookKernelIsql",
    "sql",
    "notebooks",
    "duckdb",
    "ducklab",
    "pyspark"
  ],
  "categories": [
    "Data Science",
    "Notebooks"
  ],
  "extensionDependencies": [
    "vscode.sql",
    "vscode.python"
  ],
  "type": "commonjs",
  "main": "./out/index.js",
  "activationEvents": [
    "onNotebook:ipynb-ducklab",
    "onNotebook:isql"
  ],
  "contributes": {
    "notebooks": [
      {
        "type": "isql",
        "displayName": "Ducklab",
        "selector": [
          {
            "filenamePattern": "*.isql"
          }
        ]
      }
    ],
    "commands": [
      {
        "command": "ducklab.importDatabricksPy",
        "title": "Import Databricks Noteboook",
        "category": "DuckLab"
      }
    ],
    "menus": {
      "notebook/toolbar": [],
      "explorer/context": [
        {
          "when": "resourceExtname == '.py'",
          "command": "ducklab.importDatabricksPy",
          "group": "ducklab"
        }
      ]
    }
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@rollup/plugin-alias": "^5.1.0",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/node": "^22.5.0",
    "@types/vscode": "1.85.0",
    "@types/vscode-notebook-renderer": "^1.72.3",
    "@vscode/jupyter-extension": "^1.1.1",
    "@vscode/python-extension": "1.0.5",
    "@vscode/vsce": "^3.2.1",
    "jest": "^29.7.0",
    "rollup": "^4.20.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-delete": "^3.0.1",
    "rollup-plugin-dts": "^6.2.3",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.0",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "@duckdb/node-api": "1.3.2-alpha.26",
    "@duckdb/node-bindings": "1.3.2-alpha.26",
    "@duckdb/node-bindings-win32-x64": "1.3.2-alpha.26",
    "@duckdb/node-bindings-linux-x64": "1.3.2-alpha.26",
    "@duckdb/node-bindings-linux-arm64": "1.3.2-alpha.26",
    "@duckdb/node-bindings-darwin-x64": "1.3.2-alpha.26",
    "@duckdb/node-bindings-darwin-arm64": "1.3.2-alpha.26",
    "@ducklab/core": "../core/ducklab-core-0.4.0.tgz",
    "@nteract/messaging": "7.0.*",
    "apache-arrow": "17.0.*",
    "get-port": "7.1.0",
    "vscode-notebook-error-overlay": "^1.0.1",
    "vscode-uri": "3.1.*",
    "zeromq": "6.5.0"
  }
}
